{
  "name": "shouldilistenit",
  "version": "0.0.1",
  "description": "Should I listen set you up for your next Gig.",
  "author": "Luca Lischetti <sirlisko@gmail.com> (http://sirlisko.com)",
  "repository": {
    "type": "git",
    "url": "git://github.com/sirLisko/sirLisko/shouldilistenit.git"
  },
  "scripts": {
    "forever": "forever start -a -o out.log -e error.log --uid shouldilistenit bin/www",
    "build": "run-p build:*",
    "build:js": "webpack -p",
    "build:other": "gulp",
    "lint": "eslint .",
    "start": "node bin/www",
    "postinstall": "npm run build",
    "test": "tape test/*.js | faucet",
    "watch": "run-p watch:*",
    "watch:js": "webpack -w"
  },
  "dependencies": {
    "async": "^1.5.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.13.1",
    "debug": "^2.2.0",
    "eslint": "^4.6.1",
    "express": "^4.12.4",
    "hjs": "~0.0.6",
    "ipware": "0.0.8",
    "lodash": "^4.6.1",
    "morgan": "^1.6.0",
    "nice-date": "^0.1.12",
    "npm-run-all": "^4.1.1",
    "request": "^2.65.0",
    "robots.txt": "^1.1.0",
    "serve-favicon": "^2.3.0",
    "setlistfm-parser": "git://github.com/sirLisko/setlistfm-parser.git",
    "webpack": "^3.5.6"
  },
  "devDependencies": {
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "faucet": "0.0.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-csso": "^1.0.0",
    "gulp-load-plugins": "^1.0.0-rc",
    "gulp-sass": "^2.0.1",
    "gulp-util": "^3.0.5",
    "nock": "^7.2.2",
    "tape": "^4.2.2"
  },
  "keywords": [
    "music",
    "gig",
    "live"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
